"UnlitGeneric"
{
	"$basetexture" "sprites/circular/orange_swirl"
	// the texture isn't flipped so we have to do it here
	"$basetexturetransform" "center .5 .5 scale -1 1 rotate 0 translate 0 0"
	"$additive" "1"
	"$translucent" "1"
	"$frame" "0"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedTextureVar" "$basetexture"
			"animatedTextureFrameNumVar" "$frame"
			"animatedTextureFrameRate" "15"
		}
	}
}